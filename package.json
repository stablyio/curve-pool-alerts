{
  "name": "curve-pool-alerts",
  "version": "1.0.0",
  "description": "AWS Lambda bot for Curve pool alerts",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "node build.js",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "local": "npm run build && node -e \"require('./dist/index.js').handler({ time: '2023-01-01T00:00:00Z', detail: {} })\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/aws-lambda": "^8.10.126",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "aws-cdk": "^2.106.1",
    "aws-cdk-lib": "^2.106.1",
    "constructs": "^10.3.0",
    "esbuild": "^0.19.5",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@slack/webhook": "^7.0.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1499.0",
    "axios": "^1.7.9"
  }
}
